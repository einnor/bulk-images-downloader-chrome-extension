{
  "name": "Bulk Images Downloader",
  "description": "Download every image on a web page",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "manifest_version": 2
}